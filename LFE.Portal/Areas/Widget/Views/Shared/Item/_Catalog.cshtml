@model LFE.DataTokens.IndexModelViewToken

<div class="pager-container">    
    @Html.Partial("~/Areas/Widget/Views/Shared/Layout/_Paging.cshtml")
</div>

<div class="row">
    @if (!Model.ItemsList.Any())
    {
        <h1>Courses not found in this category</h1>
    }
    else
    {
        <div id="cat-list-container">
            <ul class="ul-w-item-list">
                @{
                    for (var i = 0; i < Model.ItemsList.Count(); i++)
                    {
                        <li class="col-xs-12 col-sm-6 col-md-4 col-lg-3">                            
                            @Html.Partial("~/Areas/Widget/Views/Shared/Item/_WidgetItemBox.cshtml", Model.ItemsList[i])
                        </li>
                    }
                }
            </ul>
        </div>
    }
</div>